{
  "name": "zoekt-code-search",
  "version": "1.0.0",
  "description": "VSCode extension for efficient code searching using zoekt",
  "main": "out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "keywords": ["code search", "zoekt", "vscode extension"],
  "author": "",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:zoekt-code-search.index", "onCommand:zoekt-code-search.search"],
  "contributes": {
    "commands": [
      {
        "command": "zoekt-code-search.index",
        "title": "Zoekt: Index Current Workspace"
      },
      {
        "command": "zoekt-code-search.search",
        "title": "Zoekt: Search Code"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.1.3"
  }
}
